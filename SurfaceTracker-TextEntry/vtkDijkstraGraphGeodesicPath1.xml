<ServerManagerConfiguration>
  <ProxyGroup name="filters">
   <SourceProxy name="DijkstraGraphGeodesicPath" class="vtkDijkstraGraphGeodesicPath1" label="My DGGP">
     <Documentation
        long_help="Shortest path calculation between two points."
        short_help="shortest path.">
     </Documentation>
     <InputProperty
        name="Input"
        command="SetInputConnection">
           <ProxyGroupDomain name="groups">
             <Group name="sources"/>
             <Group name="filters"/>
           </ProxyGroupDomain>
           <DataTypeDomain name="input_type">
             <DataType value="vtkDataSet"/>
           </DataTypeDomain>
      </InputProperty>

      <DoubleVectorProperty
         name="StartVertex"
         label="Start Vertex"
         command="SetStartVertex"
         number_of_elements="1"
         animateable="1"
         default_values="0" >
         <Documentation>
           Define start point. Default is 0.
         </Documentation>
      </DoubleVectorProperty>

      <DoubleVectorProperty
         name="EndVertex"
         label="End Vertex"
         command="SetEndVertex"
         number_of_elements="1"
         animateable="1"
         default_values="10" >
         <Documentation>
           Define end point. Default is 10.
         </Documentation>
       </DoubleVectorProperty>

       <IntVectorProperty
          name="LineType"
          command="SetLineType"
          number_of_elements="1"
          default_values="0" >
        <EnumerationDomain name="enum">
          <Entry value="0" text="Geodesic"/>
          <Entry value="1" text="Sulcus"/>
          <Entry value="2" text="Gyrus"/>
        </EnumerationDomain>
        <Documentation>
          This propery specifies which type of curvature to compute.
        </Documentation>
      </IntVectorProperty>


   </SourceProxy>
 </ProxyGroup>
</ServerManagerConfiguration>
